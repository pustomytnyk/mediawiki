{
	"extends": [
		"../../.eslintrc.json",
		"plugin:qunit/recommended",
		"plugin:qunit/two"
	],
	"env": {
		"qunit": true
	},
	"globals": {
		"sinon": false
	},
	"rules": {
		"operator-linebreak": 0,
		"quote-props": [ "error", "as-needed" ],
		"valid-jsdoc": 0,
		"qunit/no-assert-equal": "error",
		"qunit/no-early-return": "error",
		"qunit/no-negated-ok": "error",
		"qunit/require-expect": 0,
		"qunit/resolve-async": 0
	},
	"plugins": [ "qunit" ]
}
